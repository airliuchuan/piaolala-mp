/* pages/express/express.wxss */
.header{
  position: relative;
}
.header .main{
  position: absolute;
  left:0;
  top:0;
  width:100%;
  height:100%;
  box-sizing: border-box;
  padding: 0 45rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color:#fff;
}
.header image{
  width:100%;
  height:180rpx;
}
.express-num{
  font-size: 28rpx
}
.header .status{
  font-size: 40rpx;
  color:#fff;
  font-weight: bold;
}